<%= render 'list', user: @user %>

<%= render 'books/list', book: @book_new %>

<h2>Books</h2>
<% @books.each do |book| %>
<%= image_tag @user.get_profile_image(100,100) %>
<div>
  <%= link_to book_path(book.id) do %>
    <p>Title</p>
    <p><%= book.title %></p>
  <% end %>
  <p>Option</p>
  <p><%= book.body %></p>
</div>
<% end %>

